    "test_tg": "export NODE_ENV=prod-test && nodemon --watch ./telegram ./telegram/index.ts",
    "start": "node index.js",
    "pooling": "export NODE_ENV=pooling && node index.js",
    "build": "rollup -c",
    "eslint_fix": "eslint . --fix --ext .ts",
    "prepare": "husky install",
    "test_telegram": "NODE_ENV=test jest --verbose ./telegram",
    "test_server": "NODE_ENV=test jest --verbose ./server"
